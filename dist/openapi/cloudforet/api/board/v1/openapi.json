           "content": {
             "application/json": {
               "schema": {
                 "properties": {"board_id" : {"type": "string","title": "BOARD_ID" },"category" : {"type": "string","title": "CATEGORY" },"title" : {"type": "string","title": "TITLE" },"contents" : {"type": "string","title": "CONTENTS" },"options" : {"type": "object","title": "OPTIONS" },"writer" : {"type": "string","title": "WRITER" },"files" : {"items": {}, "type": "array","title": "FILES" },"domain_id" : {"type": "string","title": "DOMAIN_ID" }                 }
               },
